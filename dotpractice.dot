digraph G {
    rankdir=LR
    splines=line

    node [fixedsize=true, label=""];

    subgraph cluster0 {
        color=white;
        node [style=solid, color=blue4, shape=circle];
        x1, x2, x3;
        label="Input Layer";
    }

    subgraph cluster1 {
        color=white;
        node [style=solid, color=green, shape=circle];
