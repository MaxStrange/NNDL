    }

    x1 -> h1;
    x1 -> h2;
    x1 -> h3;
    x2 -> h1;
    x2 -> h2;
    x2 -> h3;
    x3 -> h1;
    x3 -> h2;
    x3 -> h3;

    h1 -> y;
    h2 -> y;
    h3 -> y;
