import n;
import s;

network
    neurons
        layer : input 3x1 = n;
        layer : hidden 2x1 = n;
        layer : output 2x1 = n;
    end neurons

    connections
        (0, 0) : connect (0, 1), (1, [ALL]) = s;
        (0, {j < 3 && j > 0}) : connect (0, j + 1), (0, j - 1) = s;
        (0, 2) : connect (0, j - 1) = s;
        (1, 0) : connect (1, 1), (2, 0) = s;
        (1, 1) : connect (2, [ALL]) = s;
    end connections
end network
